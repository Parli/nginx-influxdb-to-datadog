{
    "name": "parli/nginx-influxdb-to-datadog",
    "license": "MIT",
    "config": {
      "sort-packages": true
    },
    "authors": [
        {
            "name": "Eric Stern",
            "email": "eric@slant.co"
        }
    ],
    "autoload": {
      "psr-4": {
        "Slant\\Monitoring\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Slant\\Monitoring\\": "tests"
      }
    },
    "require": {
        "php": "^8.0",
        "ext-sockets": "*",
        "react/datagram": "^1.4",
        "react/event-loop": "^1.0",
        "datadog/php-datadogstatsd": "^1.5"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.99",
        "phpstan/phpstan-phpunit": "^0.12.22",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "scripts": {
      "test": [
        "@phpunit",
        "@phpstan",
        "@phpcs"
      ],
      "phpunit": "phpunit",
      "phpstan": "phpstan analyse",
      "phpcs": "phpcs ."
    }
}
